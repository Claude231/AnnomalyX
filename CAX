-- SEÃ‘ALES
-- ============================================
getgenv().AX_CAHandlesAimbot = true
getgenv().AX_CAHandlesESP = true
getgenv().AX_PF_HandlesESP = true

-- ============================================
-- LOOP PRINCIPAL
-- ============================================
RunService.Heartbeat:Connect(function()
    Camera = Workspace.CurrentCamera
    FrameCounter = FrameCounter + 1

    -- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    -- FIX Bug #4: Target update CADA frame
    -- Ya no hay "if FrameCounter % 2 == 0"
    -- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    if SilentAimActive
        and Config.Silent.Enabled
        and Config.Silent.Active then
        local ok, result = pcall(FindTarget)
        if ok then
            CurrentTargetPart = result
        else
            CurrentTargetPart = nil
        end
    else
        CurrentTargetPart = nil
    end

    -- FOV Circle
    if Config.FOV
        and Config.FOV.Enabled
        and Config.FOV.Visible then
        local mp = UserInputService:GetMouseLocation()
        FOVCircle.Position = mp
        FOVCircle.Radius = Config.FOV.Radius or 400
        FOVCircle.Visible = true
    else
        FOVCircle.Visible = false
    end

    -- Target dot
    if CurrentTargetPart
        and CurrentTargetPart.Parent
        and Camera then
        local sp, on = Camera:WorldToViewportPoint(
            CurrentTargetPart.Position
        )
        if on then
            TargetDot.Position = Vector2.new(sp.X, sp.Y)
            TargetDot.Visible = true
        else
            TargetDot.Visible = false
        end
    else
        TargetDot.Visible = false
    end

    pcall(UpdateAimbot)
    pcall(UpdateCAESP)
end)

