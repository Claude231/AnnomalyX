    task.wait(0.5)
    Camera = Workspace.CurrentCamera
    CurrentTargetPart = nil
    AimTarget = nil
    AimLockedPlayer = nil
    isMouseDown = false
    CachedCharacters = {}
end)

Players.PlayerRemoving:Connect(function(player)
    RemoveCAESP(player)
    CachedCharacters[player] = nil
end)

